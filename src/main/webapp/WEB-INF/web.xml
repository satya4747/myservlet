<web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	version="3.0">
	<display-name>To do List</display-name>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
	
	<servlet>
		<servlet-name>HelloServlet</servlet-name>
		<servlet-class>com.training.HelloServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>HelloServlet</servlet-name>
		<url-pattern>/HelloServlet</url-pattern>
	</servlet-mapping>
	
	<filter>
		<filter-name>HelloServlet</filter-name>
		<filter-class>com.training.HelloServletFilter</filter-class>
	</filter>
	
	<filter-mapping>
		<filter-name>HelloServlet</filter-name>
		<url-pattern>/HelloServlet</url-pattern>
	</filter-mapping>
	
	<filter>
		<filter-name>HelloServlet2</filter-name>
		<filter-class>com.training.HelloServletFilter2</filter-class>
	</filter>
	
	<filter-mapping>
		<filter-name>HelloServlet2</filter-name>
		<url-pattern>/HelloServlet</url-pattern>
	</filter-mapping>
	
	
</web-app>